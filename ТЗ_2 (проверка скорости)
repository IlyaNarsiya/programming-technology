@Test
void testPerformance() {
    List<Integer> largeNumbers = Arrays.asList(new Integer[1000000]);
    Arrays.fill(largeNumbers, 1);
    long start, end;

    start = System.nanoTime();
    NumberProcessor._min(largeNumbers);
    end = System.nanoTime();
    System.out.println("Min function took: " + (end - start) + " ns");

    start = System.nanoTime();
    NumberProcessor._max(largeNumbers);
    end = System.nanoTime();
    System.out.println("Max function took: " + (end - start) + " ns");

    start = System.nanoTime();
    NumberProcessor._sum(largeNumbers);
    end = System.nanoTime();
    System.out.println("Sum function took: " + (end - start) + " ns");

    start = System.nanoTime();
    NumberProcessor._mult(largeNumbers);
    end = System.nanoTime();
    System.out.println("Mult function took: " + (end - start) + " ns");
}
